<Keyboard>
    <Name>Gaming Keyboard</Name>
    <HideFromKeyboardMenu>True</HideFromKeyboardMenu>
    <IsShiftAware>False</IsShiftAware>
    <ShowOutputPanel>False</ShowOutputPanel>
    <PersistNewState>False</PersistNewState>  
	<WindowState>Floating</WindowState>
	<Position>Bottom</Position> 
    <Width>100%</Width>
    <Height>33%</Height>
    <SymbolMargin>0</SymbolMargin>
    <BorderThickness>1,1,1,1</BorderThickness>
    <Grid>
        <Rows>4</Rows>
        <Cols>12</Cols>
    </Grid>
    <Keys>
