        </ChangeKeyboardKey>
        <ActionKey>
            <Action>LeftShift</Action>
            <Symbol>ShiftIcon</Symbol>
            <Row>3</Row>
            <Col>0</Col>
        </ActionKey>
        <TextKey>
            <ShiftDownLabel>&gt;</ShiftDownLabel>
            <ShiftUpLabel>.</ShiftUpLabel>
            <Text>.</Text>
            <Row>3</Row>
            <Col>1</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>&lt;</ShiftDownLabel>
            <ShiftUpLabel>,</ShiftUpLabel>
            <Text>,</Text>
            <Row>3</Row>
            <Col>2</Col>
        </TextKey>
        <TextKey>
            <Label>'</Label>
            <Text>'</Text>
            <Row>3</Row>
            <Col>3</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>:</ShiftDownLabel>
            <ShiftUpLabel>;</ShiftUpLabel>
            <Text>;</Text>
            <Row>3</Row>
            <Col>4</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>?</ShiftDownLabel>
            <ShiftUpLabel>/</ShiftUpLabel>
            <Text>/</Text>
            <Row>3</Row>
            <Col>5</Col>
        </TextKey>
        <TextKey>
            <Symbol>SpaceIcon</Symbol>
            <Text>&#32;</Text>
            <Row>3</Row>
            <Col>6</Col>
        </TextKey>
        <ActionKey>
            <Action>ArrowLeft</Action>
            <Symbol>LeftArrowKeyIcon</Symbol>
