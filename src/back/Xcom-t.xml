<Keyboard>
    <Name>XCOM B</Name>
    <HideFromKeyboardMenu>True</HideFromKeyboardMenu>
    <PersistNewState>False</PersistNewState>  
	<WindowState>Floating</WindowState>
	<Position>Top</Position> 
    <Width>100%</Width>
    <Height>20%</Height>
    <SymbolMargin>0</SymbolMargin>
    <BorderThickness>1,1,1,1</BorderThickness>
    <Grid>
        <Rows>2</Rows>
        <Cols>14</Cols>
    </Grid>
    <Keys>
		<ChangeKeyboardKey> 
            <Row>0</Row>
            <Col>0</Col>
            <Symbol>AlphaIcon</Symbol>
            <ReturnToThisKeyboard>True</ReturnToThisKeyboard>
            <DestinationKeyboard>Full-t</DestinationKeyboard>
        </ChangeKeyboardKey>
        <ActionKey>
            <Action>MouseMoveTo</Action>
            <Symbol>MouseMoveToIcon</Symbol>
		    <Label>Mouse Move</Label>
            <Row>0</Row>
            <Col>1</Col>
        </ActionKey>
        <ActionKey>
            <Action>MouseLeftClick</Action>
            <Symbol>MouseLeftClickIcon</Symbol>
	        <Label>Left Click</Label>
            <Row>0</Row>
            <Col>2</Col>
        </ActionKey>
        <ActionKey>
            <Action>MouseRightClick</Action>
            <Symbol>MouseRightClickIcon</Symbol>
			<Label>Right Click</Label>
            <Row>0</Row>
            <Col>3</Col>
        </ActionKey>
        <TextKey>
            <Symbol>SpaceIcon</Symbol>
            <Text>&#32;</Text>
            <Row>0</Row>
            <Col>4</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>Y</ShiftDownLabel>
            <ShiftUpLabel>y</ShiftUpLabel>
            <Text>y</Text>
            <Row>0</Row>
            <Col>5</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>R</ShiftDownLabel>
            <ShiftUpLabel>r</ShiftUpLabel>
            <Text>r</Text>
            <Row>0</Row>
            <Col>6</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>!</ShiftDownLabel>
            <ShiftUpLabel>1</ShiftUpLabel>
            <Text>1</Text>
            <Row>0</Row>
            <Col>7</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>@</ShiftDownLabel>
            <ShiftUpLabel>2</ShiftUpLabel>
            <Text>2</Text>
            <Row>0</Row>
            <Col>8</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>#</ShiftDownLabel>
            <ShiftUpLabel>3</ShiftUpLabel>
            <Text>3</Text>
            <Row>0</Row>
            <Col>9</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>$</ShiftDownLabel>
            <ShiftUpLabel>4</ShiftUpLabel>
            <Text>4</Text>
            <Row>0</Row>
            <Col>10</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>%</ShiftDownLabel>
            <ShiftUpLabel>5</ShiftUpLabel>
            <Text>5</Text>
            <Row>0</Row>
            <Col>11</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>^</ShiftDownLabel>
            <ShiftUpLabel>6</ShiftUpLabel>
            <Text>6</Text>
            <Row>0</Row>
            <Col>12</Col>
        </TextKey>
		<ChangeKeyboardKey> 
            <Row>0</Row>
            <Col>13</Col>
            <Symbol>ExpandDockIcon</Symbol>
            <ReturnToThisKeyboard>True</ReturnToThisKeyboard>
            <DestinationKeyboard>Xcom-b</DestinationKeyboard>
        </ChangeKeyboardKey>

        <TextKey>
            <Symbol>TabIcon</Symbol>
            <Text>&#9;</Text>
            <Row>1</Row>
            <Col>0</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>W</ShiftDownLabel>
            <ShiftUpLabel>w</ShiftUpLabel>
            <Text>w</Text>
            <Row>1</Row>
            <Col>1</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>A</ShiftDownLabel>
            <ShiftUpLabel>a</ShiftUpLabel>
            <Text>a</Text>
            <Row>1</Row>
            <Col>2</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>S</ShiftDownLabel>
            <ShiftUpLabel>s</ShiftUpLabel>
            <Text>s</Text>
            <Row>1</Row>
            <Col>3</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>D</ShiftDownLabel>
            <ShiftUpLabel>d</ShiftUpLabel>
            <Text>d</Text>
            <Row>1</Row>
            <Col>4</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>Q</ShiftDownLabel>
            <ShiftUpLabel>q</ShiftUpLabel>
            <Text>q</Text>
            <Row>1</Row>
            <Col>5</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>E</ShiftDownLabel>
            <ShiftUpLabel>e</ShiftUpLabel>
            <Text>e</Text>
            <Row>1</Row>
            <Col>6</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>F</ShiftDownLabel>
            <ShiftUpLabel>f</ShiftUpLabel>
            <Text>f</Text>
            <Row>1</Row>
            <Col>7</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>C</ShiftDownLabel>
            <ShiftUpLabel>c</ShiftUpLabel>
            <Text>c</Text>
            <Row>1</Row>
            <Col>8</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>G</ShiftDownLabel>
            <ShiftUpLabel>g</ShiftUpLabel>
            <Text>g</Text>
            <Row>1</Row>
            <Col>9</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>T</ShiftDownLabel>
            <ShiftUpLabel>t</ShiftUpLabel>
            <Text>t</Text>
            <Row>1</Row>
            <Col>10</Col>
        </TextKey>
        <TextKey>
            <ShiftDownLabel>P</ShiftDownLabel>
            <ShiftUpLabel>p</ShiftUpLabel>
            <Text>p</Text>
            <Row>1</Row>
            <Col>11</Col>
        </TextKey>
        <ActionKey>
            <Action>Escape</Action>
            <Label>Esc</Label>
            <Row>1</Row>
            <Col>12</Col>
        </ActionKey>
        <ActionKey>
            <Action>LeftCtrl</Action>
            <Label>Ctrl</Label>
            <Row>1</Row>
            <Col>13</Col>
        </ActionKey>
    </Keys>
</Keyboard>