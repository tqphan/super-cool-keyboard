      <Text>=</Text>
      <KeyUp>LCONTROL</KeyUp>
    </DynamicKey>
    <DynamicKey Row="1" Col="6">
      <Label>‚ûñ</Label>
      <KeyDown>LCONTROL</KeyDown>
      <Text>-</Text>
      <KeyUp>LCONTROL</KeyUp>
    </DynamicKey>
    <DynamicKey Row="1" Col="7">
      <Symbol>ArrowPointingToLeftIcon</Symbol>
      <KeyDown>LCONTROL</KeyDown>
      <KeyDown>LSHIFT</KeyDown>
      <KeyDown>TAB</KeyDown>
      <KeyUp>TAB</KeyUp>
      <KeyUp>LSHIFT</KeyUp>
      <KeyUp>LCONTROL</KeyUp>
    </DynamicKey>
    <DynamicKey Row="1" Col="8">
      <Symbol>ArrowPointingToRightIcon</Symbol>
      <KeyDown>LCONTROL</KeyDown>
      <KeyDown>TAB</KeyDown>
      <KeyUp>TAB</KeyUp>
      <KeyUp>LCONTROL</KeyUp>
    </DynamicKey>
    <DynamicKey Row="1" Col="9">
      <Label>üóê</Label>
      <KeyDown>LCONTROL</KeyDown>
      <Text>t</Text>
      <KeyUp>LCONTROL</KeyUp>
    </DynamicKey>
    <DynamicKey Row="1" Col="10">
      <Label>‚åß</Label>
      <KeyDown>LCONTROL</KeyDown>
      <Text>w</Text>
