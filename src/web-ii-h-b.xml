    </DynamicKey>
    <DynamicKey Row="1" Col="11">
      <Label>‚Ü∂</Label>
      <KeyDown>LCONTROL</KeyDown>
      <KeyDown>LSHIFT</KeyDown>
      <Text>t</Text>
      <KeyUp>LSHIFT</KeyUp>
      <KeyUp>LCONTROL</KeyUp>
    </DynamicKey>
  </Content>
</Keyboard>