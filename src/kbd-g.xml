<Keyboard>
  <Name>New World</Name>
  <Height>100%</Height>
  <SymbolMargin>12</SymbolMargin>
  <BorderThickness>0,0,0,0</BorderThickness>
  <DockSize>Full</DockSize>
  <Position>Right</Position>
  <Width>462</Width>
  <HorizontalOffset>0</HorizontalOffset>
  <VerticalOffset>0</VerticalOffset>
  <BackgroundColor>#ffffff</BackgroundColor>
  <BorderColor>#ffffff</BorderColor>
  <WindowState>Floating</WindowState>
  <Grid>
    <Rows>7</Rows>
    <Cols>3</Cols>
  </Grid>
  <HideFromKeyboardMenu>true</HideFromKeyboardMenu>
  <PersistNewState>false</PersistNewState>
  <Content>
    <DynamicKey row="0" Col="0">
      <Label>Shft</Label>
      <KeyToggle>LeftShift</KeyToggle>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey row="0" Col="1">
      <Label>Ctrl</Label>
      <KeyToggle>LeftCtrl</KeyToggle>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey row="0" Col="2">
      <Label>Alt</Label>
      <KeyToggle>LeftAlt</KeyToggle>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey row="1" Col="0">
      <Label>Win</Label>
      <KeyToggle>LeftWin</KeyToggle>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey row="1" Col="1">
      <Label>Tab</Label>
      <KeyDown>TAB</KeyDown>
      <KeyUp>TAB</KeyUp>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey row="1" Col="2">
      <ShiftDownLabel>K</ShiftDownLabel>
      <Label>k</Label>
      <Text>k</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey row="2" Col="0">
      <ShiftDownLabel>E</ShiftDownLabel>
      <Label>e</Label>
      <Text>e</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey row="2" Col="1">
      <ShiftDownLabel>!</ShiftDownLabel>
      <Label>1</Label>
      <Text>1</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey row="2" Col="2">
      <ShiftDownLabel>@</ShiftDownLabel>
      <Label>2</Label>
      <Text>2</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey row="3" Col="0">
      <ShiftDownLabel>Q</ShiftDownLabel>
      <Label>q</Label>
      <Text>q</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey row="3" Col="1">
      <ShiftDownLabel>R</ShiftDownLabel>
      <Label>r</Label>
      <Text>r</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey row="3" Col="2">
      <ShiftDownLabel>F</ShiftDownLabel>
      <Label>f</Label>
      <Text>f</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="6" Col="0">
        <Symbol>MinimiseIcon</Symbol>
        <ChangeKeyboard BackReturnsHere="true">mini-top-right</ChangeKeyboard>
    </DynamicKey>
    <DynamicKey Row="6" Col="1">
        <Symbol>MinimiseIcon</Symbol>
        <ChangeKeyboard BackReturnsHere="true">gaze-bar-top</ChangeKeyboard>
    </DynamicKey>
    <DynamicKey Row="6" Col="2">
      <Symbol>MenuIcon</Symbol>
      <Action>MenuKeyboard</Action>
    </DynamicKey>
  </Content>
</Keyboard>