<Keyboard>
  <Name>OptiKey-HorizontalBoard</Name>
  <Height>50%</Height>
  <SymbolMargin>12</SymbolMargin>
  <BorderThickness>0,0,0,0</BorderThickness>
  <DockSize>Full</DockSize>
  <Position>Top</Position>
  <Width>100%</Width>
  <HorizontalOffset>0</HorizontalOffset>
  <VerticalOffset>0</VerticalOffset>
  <BackgroundColor>#ffffff</BackgroundColor>
  <BorderColor>#ffffff</BorderColor>
  <WindowState>Floating</WindowState>
  <Grid>
    <Rows>4</Rows>
    <Cols>12</Cols>
  </Grid>
  <HideFromKeyboardMenu>true</HideFromKeyboardMenu>
  <PersistNewState>false</PersistNewState>
  <!-- <KeyGroup Name="All" BackgroundColor="#333333" KeyDisabledBackground="gray" KeyDownBackground="blue" /> -->
  <KeyGroup Name="Modifier" BackgroundColor="#2a2640" ForegroundColor = "#ffffff" KeyDisabledBackground="gray" KeyDownBackground="blue" />
  <KeyGroup Name="Navigation" BackgroundColor="DarkSlateGray" KeyDisabledBackground="gray" KeyDownBackground="blue" />
  <KeyGroup Name="Alphabet" BackgroundColor="#333333" ForegroundColor = "#ffffff" KeyDisabledBackground="gray" KeyDownBackground="blue" />
  <KeyGroup Name="Numeric" BackgroundColor="#2a2640" ForegroundColor = "#ffffff" KeyDisabledBackground="gray" KeyDownBackground="blue" />
  <KeyGroup Name="Edit" BackgroundColor="#2a2640" ForegroundColor = "#ffffff" KeyDisabledBackground="gray" KeyDownBackground="blue" />
  <KeyGroup Name="Punctuation" BackgroundColor="#3F3F3F" ForegroundColor = "#ffffff" KeyDisabledBackground="gray" KeyDownBackground="blue" />
  <KeyGroup Name="Pagination" BackgroundColor="#10394c" ForegroundColor = "#ffffff" KeyDisabledBackground="gray" KeyDownBackground="blue" />
  <KeyGroup Name="Exit" BackgroundColor="#0c240c" ForegroundColor = "#981220" KeyDisabledBackground="gray" KeyDownBackground="blue" />
  <KeyGroup Name="Mirror" BackgroundColor="#0c240c" ForegroundColor = "#ffffff" KeyDisabledBackground="gray" KeyDownBackground="blue" />
  <Content>
    <DynamicKey Row="0" Col="0">
      <Label>Shft</Label>
      <Action>LeftShift</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="1">
      <ShiftDownLabel>Q</ShiftDownLabel>
      <Label>q</Label>
      <Text>q</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="2">
      <ShiftDownLabel>W</ShiftDownLabel>
      <Label>w</Label>
      <Text>w</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="3">
      <ShiftDownLabel>E</ShiftDownLabel>
      <Label>e</Label>
      <Text>e</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="4">
      <ShiftDownLabel>R</ShiftDownLabel>
      <Label>r</Label>
      <Text>r</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="5">
      <ShiftDownLabel>T</ShiftDownLabel>
      <Label>t</Label>
      <Text>t</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="6">
      <ShiftDownLabel>Y</ShiftDownLabel>
      <Label>y</Label>
      <Text>y</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="7">
      <ShiftDownLabel>U</ShiftDownLabel>
      <Label>u</Label>
      <Text>u</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="8">
      <ShiftDownLabel>I</ShiftDownLabel>
      <Label>i</Label>
      <Text>i</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="9">
      <ShiftDownLabel>O</ShiftDownLabel>
      <Label>o</Label>
      <Text>o</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="10">
      <ShiftDownLabel>P</ShiftDownLabel>
      <Label>p</Label>
      <Text>p</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="0" Col="11">
      <Symbol>BackOneIcon</Symbol>
      <Action>BackOne</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="0">
      <Label>Win</Label>
      <Action>LeftWin</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="1">
      <ShiftDownLabel>A</ShiftDownLabel>
      <Label>a</Label>
      <Text>a</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="2">
      <ShiftDownLabel>S</ShiftDownLabel>
      <Label>s</Label>
      <Text>s</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="3">
      <ShiftDownLabel>D</ShiftDownLabel>
      <Label>d</Label>
      <Text>d</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="4">
      <ShiftDownLabel>F</ShiftDownLabel>
      <Label>f</Label>
      <Text>f</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="5">
      <ShiftDownLabel>G</ShiftDownLabel>
      <Label>g</Label>
      <Text>g</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="6">
      <ShiftDownLabel>H</ShiftDownLabel>
      <Label>h</Label>
      <Text>h</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="7">
      <ShiftDownLabel>J</ShiftDownLabel>
      <Label>j</Label>
      <Text>j</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="8">
      <ShiftDownLabel>K</ShiftDownLabel>
      <Label>k</Label>
      <Text>k</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="9">
      <ShiftDownLabel>L</ShiftDownLabel>
      <Label>l</Label>
      <Text>l</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="10">
      <ShiftDownLabel>></ShiftDownLabel>
      <Label>.</Label>
      <Text>.</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="1" Col="11">
      <Symbol>EnterIcon</Symbol>
      <Text>&#13;</Text>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="0">
      <Label>Ctrl</Label>
      <Action>LeftCtrl</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="1">
      <Label>Alt</Label>
      <Action>LeftAlt</Action>
      <KeyGroup>Modifier</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="2">
      <ShiftDownLabel>Z</ShiftDownLabel>
      <Label>z</Label>
      <Text>z</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="3">
      <ShiftDownLabel>X</ShiftDownLabel>
      <Label>x</Label>
      <Text>x</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="4">
      <ShiftDownLabel>C</ShiftDownLabel>
      <Label>c</Label>
      <Text>c</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="5">
      <ShiftDownLabel>V</ShiftDownLabel>
      <Label>v</Label>
      <Text>v</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="6">
      <ShiftDownLabel>B</ShiftDownLabel>
      <Label>b</Label>
      <Text>b</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="7">
      <ShiftDownLabel>N</ShiftDownLabel>
      <Label>n</Label>
      <Text>n</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="8">
      <ShiftDownLabel>M</ShiftDownLabel>
      <Label>m</Label>
      <Text>m</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="9">
      <Label>‚ê£</Label>
      <Text>&#32;</Text>
      <KeyGroup>Alphabet</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="10">
      <ShiftDownLabel>&lt;</ShiftDownLabel>
      <Label>,</Label>
      <Text>,</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="2" Col="11">
      <ShiftDownLabel>?</ShiftDownLabel>
      <Label>/</Label>
      <Text>/</Text>
      <KeyGroup>Punctuation</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="3" Col="0">
      <Symbol>DownArrowKeyIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="false">kbd-ii-h-b</ChangeKeyboard>
      <KeyGroup>Mirror</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="3" Col="1">
      <Symbol>PhysicalKeysIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="false">kbd-i-h-t</ChangeKeyboard>
      <KeyGroup>Pagination</KeyGroup>
    </DynamicKey>
    <SuggestionRow Row="3" Col="2" Width="8">
      <KeyGroup>Exit</KeyGroup>
    </SuggestionRow>
    <DynamicKey Row="3" Col="10">
      <Symbol>NumericAndSymbolsIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="false">kbd-iii-h-t</ChangeKeyboard>
      <KeyGroup>Pagination</KeyGroup>
    </DynamicKey>
    <DynamicKey Row="3" Col="11">
      <Symbol>NoIcon</Symbol>
      <Action>BackFromKeyboard</Action>
      <!-- <ChangeKeyboard BackReturnsHere="false">gaze-bar-top</ChangeKeyboard> -->
      <KeyGroup>Exit</KeyGroup>
    </DynamicKey>
  </Content>
</Keyboard>