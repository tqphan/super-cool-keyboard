<Keyboard>
  <Name>SCK - Horizontal</Name>
  <Height>15%</Height>
  <SymbolMargin>12</SymbolMargin>
  <BorderThickness>0,0,0,0</BorderThickness>
  <DockSize>Full</DockSize>
  <Position>Bottom</Position>
  <Width>100%</Width>
  <HorizontalOffset>0</HorizontalOffset>
  <VerticalOffset>0</VerticalOffset>
  <BackgroundColor>#ffffff</BackgroundColor>
  <BorderColor>#ffffff</BorderColor>
  <WindowState>Floating</WindowState>
  <PersistNewState>false</PersistNewState>
  <Grid>
    <Rows>1</Rows>
    <Cols>12</Cols>
  </Grid>
  <Content>
    <DynamicKey Row="0" Col="0">
      <Symbol>UpArrowKeyIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="true">gaze-bar-top</ChangeKeyboard>
    </DynamicKey>
    <DynamicKey Row="0" Col="1">
      <Symbol>MouseLeftClickIcon</Symbol>
      <Action>MouseMoveAndLeftClick</Action>
    </DynamicKey>
    <DynamicKey Row="0" Col="2">
      <Symbol>MouseRightClickIcon</Symbol>
      <Action>MouseMoveAndRightClick</Action>
    </DynamicKey>
    <DynamicKey Row="0" Col="3">
      <Symbol>LookToScrollActiveIcon</Symbol>
      <Action>LookToScrollActive</Action>
    </DynamicKey>
    <DynamicKey Row="0" Col="4">
      <Symbol>ConversationIcon</Symbol>
      <Action>ConversationAlpha1Keyboard</Action>
    </DynamicKey>
    <DynamicKey Row="0" Col="5">
      <Symbol>KeyboardIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="true">kbd-ii-h-b</ChangeKeyboard>
    </DynamicKey>
    <DynamicKey Row="0" Col="6">
      <Symbol>MouseIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="true">mouse-i-h-b</ChangeKeyboard>
    </DynamicKey>
    <DynamicKey Row="0" Col="7">
      <Symbol>BrowserIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="true">web-i-h-b</ChangeKeyboard>
    </DynamicKey>
    <DynamicKey Row="0" Col="8">
      <Symbol>WinIcon</Symbol>
      <KeyDown>LWIN</KeyDown>
      <KeyUp>LWIN</KeyUp>
    </DynamicKey>
    <DynamicKey Row="0" Col="9">
      <Symbol>LookToScrollBoundsSubwindowIcon</Symbol>
      <KeyDown>LWIN</KeyDown>
      <KeyDown>TAB</KeyDown>
      <KeyUp>TAB</KeyUp>
      <KeyUp>LWIN</KeyUp>
    </DynamicKey>
    <DynamicKey Row="0" Col="10">
      <Symbol>MenuIcon</Symbol>
      <Action>MenuKeyboard</Action>
    </DynamicKey>
    <DynamicKey Row="0" Col="11">
      <Symbol>MinimiseIcon</Symbol>
      <ChangeKeyboard BackReturnsHere="true">mini-bottom</ChangeKeyboard>
    </DynamicKey>
  </Content>
</Keyboard>