<Keyboard>
  <Name>OptiKey-BoardMaker</Name>
  <Height>5%</Height>
  <SymbolMargin>0</SymbolMargin>
  <BorderThickness>0,0,0,0</BorderThickness>
  <DockSize>Full</DockSize>
  <Position>TopRight</Position>
  <Width>5%</Width>
  <HorizontalOffset>0</HorizontalOffset>
  <VerticalOffset>0</VerticalOffset>
  <BackgroundColor>Transparent</BackgroundColor>
  <BorderColor>Transparent</BorderColor>
  <WindowState>Floating</WindowState>
  <Grid>
    <Rows>1</Rows>
    <Cols>1</Cols>
  </Grid>
  <HideFromKeyboardMenu>true</HideFromKeyboardMenu>
  <KeyGroup Name="Minimize" Opacity="0.1" />
  <Content>
    <DynamicKey Row="0" Col="0">
      <Label>back</Label>
      <Action>BackFromKeyboard</Action>
      <KeyGroup>Minimize</KeyGroup>
    </DynamicKey>
  </Content>
</Keyboard>